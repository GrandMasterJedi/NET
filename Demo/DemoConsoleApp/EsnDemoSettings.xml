<?xml version="1.0" encoding="UTF-8"?>
<!-- Specify your data location in DataDir attribute -->
<EsnDemoSettings DataDir=".\\Data">
  <DemoCase Name="Sinusoid time series prediction (Random topology, Linear regression readout)">
    <Samples CsvDataFileName="Sinusoid.csv" NumOfBootSamples="400" MaxNumOfTrainingSamples="1000" NumOfTestSamples="10" TestSamplesSelectionMethod="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="false">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology 200 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="1" WeightScale="0.2"/>
          <Internal Size="200" Activation="TanH" WeightScale="0.2">
            <RandomTopology ConnectionsDensity="0.1"/>
          </Internal>
          <!-- Feedback is nonsense in this case (duplicates the input) and it is placed here only for usage demostration -->
          <Feedback Density="1" WeightScale="0.2">
            <Field Name="Value"/>
          </Feedback>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology 200 TanH neurons" AugmentedStates="false">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Linear" Attempts="1" AttemptEpochs="1" AttemptStopMSE="1E-20"/>
      <OutputFields>
        <Field Name="Value"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="Sinusoid time series prediction (Ring topology, Readout with hidden TanH neurons and Resilient trainer)">
    <Samples CsvDataFileName="Sinusoid.csv" NumOfBootSamples="100" MaxNumOfTrainingSamples="1000" NumOfTestSamples="10" TestSamplesSelectionMethod="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="true">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Ring topology 50 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="1" WeightScale="0.05"/>
          <Internal Size="50" Activation="TanH" WeightScale="0.5">
            <RingTopology BiDirection="false" SelfConnectionsDensity="0" InterConnectionsDensity="0"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Ring topology 50 TanH neurons" AugmentedStates="false">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="2" AttemptEpochs="5000" AttemptStopMSE="1E-20">
        <HiddenLayer Neurons="52" Activation="TanH"/>
      </Readout>
      <OutputFields>
        <Field Name="Value"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="MackeyGlass chaotic time series prediction (Random topology, Linear regression readout)">
    <Samples CsvDataFileName="MackeyGlass.csv" NumOfBootSamples="2000" MaxNumOfTrainingSamples="2000" NumOfTestSamples="500" TestSamplesSelectionMethod="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="false">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology 300 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="1" WeightScale="0.2"/>
          <Internal Size="300" Activation="TanH" WeightScale="0.2">
            <RandomTopology ConnectionsDensity="0.15"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology 300 TanH neurons" AugmentedStates="true">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Linear" Attempts="1" AttemptEpochs="10" AttemptStopMSE="1E-20"/>
