  <DemoCase Name="Prediction of the Sinusoid time series (Single reservoir, Hidden layers, Resilient)">
    <Samples CsvDataFileName="Sinusoid.csv" NumOfBootSamples="100" MaxNumOfTrainingSamples="1000" NumOfTestSamples="10" TestSamplesSelectionMethod="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="true">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Ring topology with 50 TanH neurons">
          <Input ConnectionDensity="1" WeightScale="0.05"/>
          <Internal Size="50" Activation="TanH" BiasScale="0" WeightScale="1">
            <RingTopology Bidirectional="false" SelfConnectionsDensity="0" InterConnectionsDensity="0"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Ring topology with 50 TanH neurons" AugmentedStates="false">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="10" AttemptEpochs="700" AttemptStopMSE="1E-20">
        <HiddenLayer Neurons="52" Activation="TanH"/>
        <HiddenLayer Neurons="52" Activation="TanH"/>
      </Readout>
      <OutputFields>
        <Field Name="Value"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
