<?xml version="1.0" encoding="UTF-8"?>
<esnDemo dataFolder=".\\Data">
  <!-- Modify/Tune existing or add your own cases -->
  <case name="Prediction task High and Low prices of SPXL (Direxion Daily SP500 Bull 3X ETF): Reservoir instance for each input field, Resilient">
    <!-- Data from https://finance.yahoo.com/quote/SPXL/history?p=SPXL -->
    <samples fileName="SPXL.csv" bootSamples="400" singleNormalizer="true" normalizerReserve="0.1"/>
    <esnCfg taskType="Prediction" randomizerSeek="0">
      <inputFields routeToReadout="true">
        <field name="High"/>
        <field name="Low"/>
        <field name="Adj Close"/>
      </inputFields>
      <reservoirCfgContainer>
        <reservoirCfg name="Random topology of 200 TanH neurons">
          <input connectionDensity="1" weightScale="0.9"/>
          <internal size="200" activation="TanH" biasScale="0" weightScale="0.2">
            <topologyRandom connectionsDensity="0.1"/>
            <retainmentNeurons density="1" retainmentMinRate="0" retainmentMaxRate="0.5"/>
          </internal>
        </reservoirCfg>
        <reservoirCfg name="DTT topology of 200 TanH neurons">
          <input connectionDensity="1" weightScale="0.9"/>
          <internal size="200" activation="TanH" biasScale="0" weightScale="0.9">
            <topologyDTT selfConnectionsDensity="0"/>
            <retainmentNeurons density="1" retainmentMinRate="0" retainmentMaxRate="0.8"/>
          </internal>
        </reservoirCfg>
      </reservoirCfgContainer>
      <reservoirInstanceContainer>
        <reservoirInstance name="High" cfg="Random topology of 200 TanH neurons" augmentedStates="false">
          <inputField name="High"/>
        </reservoirInstance>
        <reservoirInstance name="Low" cfg="Random topology of 200 TanH neurons" augmentedStates="false">
          <inputField name="Low"/>
        </reservoirInstance>
        <reservoirInstance name="Adj Close" cfg="DTT topology of 200 TanH neurons" augmentedStates="false">
          <inputField name="Adj Close"/>
        </reservoirInstance>
      </reservoirInstanceContainer>
      <readoutLayer testDataRatio="0.1" folds="Auto">
        <outputFields activation="Identity" regressionMethod="Resilient" attempts="5" attemptEpochs="200" attemptStopMSE="1E-20">
          <field name="High"/>
          <field name="Low"/>
        </outputFields>
      </readoutLayer>
    </esnCfg>
  </case>
  <case name="Prediction task High and Low prices of SPXL (Direxion Daily SP500 Bull 3X ETF): Single reservoir, Hidden layer, Resilient">
    <!-- Data from https://finance.yahoo.com/quote/SPXL/history?p=SPXL -->
    <samples fileName="SPXL.csv" bootSamples="400" singleNormalizer="true" normalizerReserve="0.1"/>
    <esnCfg taskType="Prediction" randomizerSeek="0">
      <inputFields routeToReadout="true">
        <field name="High"/>
        <field name="Low"/>
        <field name="Adj Close"/>
      </inputFields>
      <reservoirCfgContainer>
        <reservoirCfg name="DTT topology of 400 TanH neurons">
          <input connectionDensity="1" weightScale="0.5"/>
          <internal size="400" activation="TanH" biasScale="0" weightScale="0.3">
            <topologyDTT selfConnectionsDensity="1"/>
            <retainmentNeurons density="0.5" retainmentMinRate="0" retainmentMaxRate="0.75"/>
            <contextNeuron activation="TanH" inputWeight="0.05" feedbackDensity="1" feedbackWeight="0.3"/>
          </internal>
        </reservoirCfg>
      </reservoirCfgContainer>
      <reservoirInstanceContainer>
        <reservoirInstance name="Main" cfg="DTT topology of 400 TanH neurons" augmentedStates="false">
          <inputField name="High"/>
          <inputField name="Low"/>
          <inputField name="Adj Close"/>
        </reservoirInstance>
      </reservoirInstanceContainer>
      <readoutLayer testDataRatio="0.1" folds="Auto">
        <hiddenLayer neurons="10" activation="TanH"/>
        <outputFields activation="Identity" regressionMethod="Resilient" attempts="2" attemptEpochs="700" attemptStopMSE="1E-20">
          <field name="High"/>
          <field name="Low"/>
        </outputFields>
      </readoutLayer>
    </esnCfg>
  </case>
  <case name="Prediction task High and Low prices of TTOO (T2 Biosystems, Inc.): Single reservoir, Linear regression">
