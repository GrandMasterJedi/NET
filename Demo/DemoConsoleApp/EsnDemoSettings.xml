    <samples fileName="TTOO.csv" bootSamples="300" singleNormalizer="true" normalizerReserve="0.1"/>
    <esnCfg taskType="Prediction" randomizerSeek="0">
      <inputFields routeToReadout="true">
        <field name="High"/>
        <field name="Low"/>
        <field name="Adj Close"/>
      </inputFields>
      <reservoirCfgContainer>
        <reservoirCfg name="Random topology of 300 TanH neurons">
          <input connectionDensity="0.5" weightScale="0.4"/>
          <internal size="300" activation="TanH" biasScale="0" weightScale="0.4">
            <topologyRandom connectionsDensity="0.1"/>
          </internal>
        </reservoirCfg>
      </reservoirCfgContainer>
      <reservoirInstanceContainer>
        <reservoirInstance name="Main" cfg="Random topology of 300 TanH neurons" augmentedStates="false">
          <inputField name="High"/>
          <inputField name="Low"/>
          <inputField name="Adj Close"/>
        </reservoirInstance>
      </reservoirInstanceContainer>
      <readoutLayer testDataRatio="0.1" folds="Auto">
