  <DemoCase Name="Yahoo finance TTOO title High and Low prices prediction (single reservoir for all input fields, no hidden readout neurons and Resilient trainer)">
    <Samples CsvDataFileName="TTOO.csv" NumOfBootSamples="300" MaxNumOfTrainingSamples="1000" NumOfTestSamples="10" TestSamplesSelectionMethod="Random" SingleNormalizer="true" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="true">
        <Field Name="High"/>
        <Field Name="Low"/>
        <Field Name="Adj Close"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology 300 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="0.5" WeightScale="0.4"/>
          <Internal Size="300" Activation="TanH" WeightScale="0.4">
            <RandomTopology ConnectionsDensity="0.1"/>
            <RetainmentNeurons Density="0.5" RetainmentMinRate="0" RetainmentMaxRate="0.75"/>
            <ContextNeuron FeedbackDensity="1" Activation="Elliot" InWeightScale="1" OutWeightScale="0.2"/>
          </Internal>
          <Feedback Density="0" WeightScale="0"/>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology 300 TanH neurons" AugmentedStates="false">
          <InputField Name="High"/>
          <InputField Name="Low"/>
          <InputField Name="Adj Close"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="5" AttemptEpochs="10000" AttemptStopMSE="1E-20"/>
      <OutputFields>
        <Field Name="High"/>
        <Field Name="Low"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
