<?xml version="1.0" encoding="UTF-8"?>
<!-- You can specify your data location in DataDir attribute -->
<EsnDemoSettings DataDir=".\\Data">
  <!-- You can modify/remove existing or add your own DemoCase elements -->
  <DemoCase Name="Prediction of the High and Low prices of SPXL title from Yahoo finance (A separate reservoir for each input field, Resilient)">
    <Samples CsvDataFileName="SPXL.csv" NumOfBootSamples="400" NumOfTestSamples="2" TestSamplesSelectionMethod="Sequential" SingleNormalizer="true" NormalizerReserveRatio="0.1"/>
    <EsnSettings TaskType="Prediction" RandomizerSeek="0">
      <InputFields RouteToReadout="true">
        <Field Name="High"/>
        <Field Name="Low"/>
        <Field Name="Adj Close"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology with 200 TanH neurons">
          <Input ConnectionDensity="1" WeightScale="0.9"/>
          <Internal Size="200" Activation="TanH" BiasScale="0" WeightScale="0.2">
            <RandomTopology ConnectionsDensity="0.1"/>
            <RetainmentNeurons Density="1" RetainmentMinRate="0" RetainmentMaxRate="0.5"/>
          </Internal>
          <Feedback Density="0" WeightScale="0"/>
        </AnalogReservoirSettings>
        <AnalogReservoirSettings Name="DTT topology with 200 TanH neurons">
          <Input ConnectionDensity="1" WeightScale="0.9"/>
          <Internal Size="200" Activation="TanH" BiasScale="0" WeightScale="0.9">
            <DTTTopology SelfConnectionsDensity="0"/>
            <RetainmentNeurons Density="1" RetainmentMinRate="0" RetainmentMaxRate="0.8"/>
          </Internal>
          <Feedback Density="0" WeightScale="0"/>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="High" SettingsName="Random topology with 200 TanH neurons" AugmentedStates="false">
          <InputField Name="High"/>
        </ReservoirInstance>
        <ReservoirInstance InstanceName="Low" SettingsName="Random topology with 200 TanH neurons" AugmentedStates="false">
          <InputField Name="Low"/>
        </ReservoirInstance>
        <ReservoirInstance InstanceName="Adj Close" SettingsName="DTT topology with 200 TanH neurons" AugmentedStates="false">
          <InputField Name="Adj Close"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="5" AttemptEpochs="200" AttemptStopMSE="1E-20"/>
      <OutputFields>
        <Field Name="High"/>
        <Field Name="Low"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="Prediction of the High and Low prices of SPXL title from Yahoo finance (Single reservoir, Hidden layer, Resilient)">
    <Samples CsvDataFileName="SPXL.csv" NumOfBootSamples="400" NumOfTestSamples="2" TestSamplesSelectionMethod="Sequential" SingleNormalizer="true" NormalizerReserveRatio="0.1"/>
    <EsnSettings TaskType="Prediction" RandomizerSeek="0">
      <InputFields RouteToReadout="true">
        <Field Name="High"/>
        <Field Name="Low"/>
        <Field Name="Adj Close"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="DTT topology with 400 TanH neurons">
          <Input ConnectionDensity="1" WeightScale="0.5"/>
          <Internal Size="400" Activation="TanH" BiasScale="0" WeightScale="0.3">
            <DTTTopology SelfConnectionsDensity="1"/>
            <RetainmentNeurons Density="0.5" RetainmentMinRate="0" RetainmentMaxRate="0.75"/>
            <ContextNeuron Activation="TanH" InputWeight="0.05" FeedbackDensity="1" FeedbackWeight="0.3"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="DTT topology with 400 TanH neurons" AugmentedStates="false">
          <InputField Name="High"/>
          <InputField Name="Low"/>
          <InputField Name="Adj Close"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="2" AttemptEpochs="700" AttemptStopMSE="1E-20">
        <HiddenLayer Neurons="10" Activation="TanH"/>
      </Readout>
      <OutputFields>
        <Field Name="High"/>
        <Field Name="Low"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="Pattern recognition task Libras Movement from https://archive.ics.uci.edu/ml/datasets/Libras+Movement (Single reservoir, Hidden layer, Resilient)">
    <Samples CsvDataFileName="LibrasMovement.csv" NumOfTestSamples="15" TestSamplesSelectionMethod="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings TaskType="Classification" RandomizerSeek="0">
      <InputFields RouteToReadout="false">
        <Field Name="coord_abcissa"/>
        <Field Name="coord_ordinate"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology with 400 TanH neurons">
          <Input ConnectionDensity="1" WeightScale="0.2"/>
          <Internal Size="400" Activation="TanH" BiasScale="0.1" WeightScale="0.4">
            <RandomTopology ConnectionsDensity="0.1"/>
            <ContextNeuron Activation="TanH" InputWeight="0.5" FeedbackDensity="1" FeedbackWeight="0.3"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology with 400 TanH neurons" AugmentedStates="false">
          <InputField Name="coord_abcissa"/>
          <InputField Name="coord_ordinate"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="TanH" RegressionMethod="Resilient" Attempts="3" AttemptEpochs="2000" AttemptStopMSE="1E-20">
        <HiddenLayer Neurons="10" Activation="TanH"/>
      </Readout>
      <OutputFields>
        <Field Name="curved swing"/>
        <Field Name="horizontal swing"/>
        <Field Name="vertical swing"/>
        <Field Name="anti-clockwise arc"/>
        <Field Name="clockwise arc"/>
        <Field Name="circle"/>
        <Field Name="horizontal straight-line"/>
        <Field Name="vertical straight-line"/>
        <Field Name="horizontal zigzag"/>
        <Field Name="vertical zigzag"/>
        <Field Name="horizontal wavy"/>
        <Field Name="vertical wavy"/>
        <Field Name="face-up curve"/>
        <Field Name="face-down curve"/>
        <Field Name="tremble"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
