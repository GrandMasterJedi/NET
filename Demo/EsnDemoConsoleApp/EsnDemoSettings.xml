<?xml version="1.0" encoding="UTF-8"?>
<!-- Specify your data location in DataDir attribute -->
<EsnDemoSettings DataDir=".\\Data">
  <DemoCase Name="Sinusoid time series prediction (Random topology, Linear regression readout)">
    <Samples CSVDataFileName="Sinusoid.csv" BootSeqMinLength="700" TrainingSeqMaxLength="1000" TestingSeqLength="10" TestSamplesSelection="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="false">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology 200 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="1" WeightScale="0.2"/>
          <Internal Size="200" Activation="TanH" WeightScale="0.2">
            <RandomTopology ConnectionsDensity="0.1"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology 200 TanH neurons" AugmentedStates="false">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Linear" Attempts="1" AttemptEpochs="1" AttemptStopMSE="1E-20"/>
      <OutputFields>
        <Field Name="Value"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="Sinusoid time series prediction (Ring topology, Readout with hidden TanH neurons and Resilient trainer)">
    <Samples CSVDataFileName="Sinusoid.csv" BootSeqMinLength="700" TrainingSeqMaxLength="1000" TestingSeqLength="10" TestSamplesSelection="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="false">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Ring topology 200 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="1" WeightScale="0.2"/>
          <Internal Size="200" Activation="TanH" WeightScale="0.2">
            <RingTopology BiDirection="false" SelfConnectionsDensity="0.1" InterConnectionsDensity="0.1"/>
          </Internal>
          <!-- Feedback is nonsense. It is placed here only for feature usage demostration -->
          <Feedback Density="1" WeightScale="0.2">
            <Field Name="Value"/>
          </Feedback>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Ring topology 200 TanH neurons" AugmentedStates="false">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="2" AttemptEpochs="5000" AttemptStopMSE="1E-20">
        <HiddenLayer Neurons="20" Activation="TanH"/>
      </Readout>
      <OutputFields>
        <Field Name="Value"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="MackeyGlass chaotic time series prediction (Random topology, Linear regression readout)">
    <Samples CSVDataFileName="MackeyGlass.csv" BootSeqMinLength="2000" TrainingSeqMaxLength="2000" TestingSeqLength="500" TestSamplesSelection="Sequential" SingleNormalizer="false" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="false">
        <Field Name="Value"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology 300 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="1" WeightScale="0.2"/>
          <Internal Size="300" Activation="TanH" WeightScale="0.2">
            <RandomTopology ConnectionsDensity="0.15"/>
          </Internal>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology 300 TanH neurons" AugmentedStates="true">
          <InputField Name="Value"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Linear" Attempts="1" AttemptEpochs="10" AttemptStopMSE="1E-20"/>
      <OutputFields>
        <Field Name="Value"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
  <DemoCase Name="Yahoo finance TTOO title High and Low prices prediction (single reservoir for all input fields, no hidden readout neurons and Resilient trainer)">
    <Samples CSVDataFileName="TTOO.csv" BootSeqMinLength="300" TrainingSeqMaxLength="1000" TestingSeqLength="10" TestSamplesSelection="Random" SingleNormalizer="true" NormalizerReserveRatio="0.1"/>
    <EsnSettings RandomizerSeek="0">
      <InputFields RouteToReadout="true">
        <Field Name="High"/>
        <Field Name="Low"/>
        <Field Name="Adj Close"/>
      </InputFields>
      <ReservoirSettingsContainer>
        <AnalogReservoirSettings Name="Random topology 300 TanH neurons">
          <Input BiasScale="0" ConnectionDensity="0.5" WeightScale="0.4"/>
          <Internal Size="300" Activation="TanH" WeightScale="0.4">
            <RandomTopology ConnectionsDensity="0.1"/>
            <RetirementNeurons Density="0.5" RetirementMinRate="0" RetirementMaxRate="0.75"/>
            <ContextNeuron FeedbackDensity="1" Activation="Elliot" InWeightScale="1" OutWeightScale="0.2"/>
          </Internal>
          <Feedback Density="0" WeightScale="0"/>
        </AnalogReservoirSettings>
      </ReservoirSettingsContainer>
      <ReservoirInstancesContainer>
        <ReservoirInstance InstanceName="Main" SettingsName="Random topology 300 TanH neurons" AugmentedStates="false">
          <InputField Name="High"/>
          <InputField Name="Low"/>
          <InputField Name="Adj Close"/>
        </ReservoirInstance>
      </ReservoirInstancesContainer>
      <Readout OutputActivation="Identity" RegressionMethod="Resilient" Attempts="5" AttemptEpochs="10000" AttemptStopMSE="1E-20"/>
      <OutputFields>
        <Field Name="High"/>
        <Field Name="Low"/>
      </OutputFields>
    </EsnSettings>
  </DemoCase>
