          <Field Name="Adj Close"/>
        </Input>
        <Internal Size="1000" Activation="TANH" WeightScale="0.2">
          <RandomTopology ConnectionsDensity="0.1"/>
          <RetirementNeurons Density="0.5" RetirementMinRate="0" RetirementMaxRate="0.75"/>
