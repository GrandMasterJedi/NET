        </Internal>
        <Feedback Density="0" WeightScale="0"/>
      </Reservoir>
      <Readout Activation="IDENTITY">
      </Readout>
      <Regression RegressionMethod="RESILIENT" MaxAttempts="5" MaxEpochs="10000" StopMSE="1E-20"/>
    </ESN>
    <Output>
      <Field Name="High"/>
      <Field Name="Low"/>
    </Output>
  </DemoCase>
