  <DemoCase Name="SPXL High and Low prices prediction (single reservoir for all input fields, single readout neuron and RESILIENT trainer)">
    <Samples CSVDataFileName="SPXL.csv" BootSeqMinLength="1000" TrainingSeqMaxLength="2000" TestingSeqLength="10" TestSamplesSelection="RANDOM" SingleNormalizer="true" NormalizerReserveRatio="0.1"/>
    <ESN RandomizerSeek="0">
